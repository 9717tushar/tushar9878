        },
        "SESSION3": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "SESSION4": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "SESSION5": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "SESSION6": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "SESSION7": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "SESSION8": {
            "description": "Pyrogram String Session",
            "required": false
        },
        "ALIVE_PIC": {
            "description": "Alive Pic or video (telegraph link)",
            "required": false
        },
        "API_HASH": {
            "description": "my.telegram.org",
